      <Button fx:id="auctionManagementBtn" layoutX="7.0" layoutY="369.0" mnemonicParsing="false" onAction="#setAuctionManagementBtn" prefHeight="55.0" prefWidth="370.0" style="-fx-background-color: transparent;" />
